<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8">
    <title><%=title %></title>
    <link type="text/css" href="./style.css" rel="stylesheet">
  </head>
  <body>
    <!-- Get from app.js -->
    <h1><%=title %></h1>
    <div role="main">
      <!-- Get from app.js -->
      <p><%=content %></p>
      <!-- Input Form -->
      <form method="post" action="/other">
        <!-- Post transmission to "/others" -->
        <p>
          <input type="text" name="msg">
          <input type="submit" value="Click">
        </p>
      </form>
      <p>
        <table>
          <% for (var key in data) { %>
          <%- include( 'data_item',{ key: key, val: [ data[key] ] } ) -%>
          <% } %>
        </table>
      </p>
    </div>
  </body>
</html>

